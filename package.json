{
  "name": "ahnjin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test-ios:e2e": "detox test -c ios.sim.debug",
    "build-ios:e2e": "detox build -c ios.sim.debug",
    "ci-ios:test:e2e": "detox test -c ios.sim.release -l verbose --cleanup",
    "ci-ios:build:e2e": "detox build -c ios.sim.release",
    "test-android:e2e": "detox test -c android.emu.debug",
    "build-android:e2e": "detox build -c android.emu.debug",
    "ci-android:test:e2e": "detox test -c android.emu.release -l verbose --cleanup",
    "ci-android:build:e2e": "detox build -c android.emu.release",
    "format": "npm-run-all format:*",
    "format:json": "prettier --write \"app/**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"app/**/*.ts{,x}\"",
    "patch": "patch-package",
    "build-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clean": "react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "clean-package": "rm -rf package-lock.json && rm -rf node_modules &&  rm -rf ios/Pods && rm ios/Podfile.lock && npm install --force && cd ios && pod install && cd .."
  },
  "dependencies": {
    "@expo-google-fonts/space-grotesk": "^0.2.3",
    "@react-native-async-storage/async-storage": "^1.17.12",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "apisauce": "^3.0.0",
    "expo-localization": "^14.1.1",
    "i18n-js": "3.9.2",
    "ignite-cli": "^7.15.0",
    "jest-expo": "^48.0.2",
    "mobx": "^6.8.0",
    "mobx-react-lite": "^3.4.3",
    "mobx-state-tree": "^5.1.7",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.71.4",
    "react-native-clean-project": "^4.0.1",
    "react-native-flipper": "^0.184.0",
    "react-native-rename": "^3.2.12",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-query": "^3.39.3",
    "react-query-native-devtools": "^4.0.0",
    "reactotron-mst": "^3.1.4",
    "reactotron-react-js": "^3.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "^7.20.0",
    "@jest/globals": "^29.5.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/i18n-js": "3.8.2",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "babel-preset-react-native": "^4.0.1",
    "detox": "^20.5.0",
    "eslint": "^8.19.0",
    "jest": "^29.5.0",
    "jest-react-native": "^18.0.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "21.1.1",
    "jsdom-global": "3.0.2",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "reactotron-react-native": "^5.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.8.4"
  }
}
